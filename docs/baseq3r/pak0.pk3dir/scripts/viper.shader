//red

models/players/viper/red
{
	{
//		map textures/effects/tinfx2b.tga
		map textures/reflect/reflect.jpg          
		tcGen environment
		rgbGen lightingdiffuse
	}

	{
		map models/players/viper/red.tga
		blendFunc	GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
//		alphaFunc GT0
		depthWrite
		rgbGen lightingdiffuse
	}


}

