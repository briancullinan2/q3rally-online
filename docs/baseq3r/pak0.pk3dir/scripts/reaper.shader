//REAPER SHADER SCRIPTS

//REAPER SHARED

models/players/reaper/net
{

	{
		map models/players/reaper/parts.tga
		blendFunc	GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		alphaFunc GT0
		depthWrite
		rgbGen lightingdiffuse
	}
}

//REAPER COLORS

