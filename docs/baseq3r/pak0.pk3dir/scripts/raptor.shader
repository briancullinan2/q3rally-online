//RAPTOR SHADER SCRIPTS


models/players/raptor/hlites
{
  	{
	  	map models/players/raptor/hlites.tga
		depthWrite
		rgbGen lightingdiffuse
	}
      {
		map models/players/raptor/hlites_g.tga
            blendFunc add
		rgbGen identity
	}    
}


