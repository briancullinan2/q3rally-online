	{
		map textures/reflect/reflect.jpg
                tcGen environment
		rgbGen identity
		
	}
	{
		map textures/q3r_building/stuccowin01.tga
		blendFunc	GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen identity
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc filter
	}
        
}

