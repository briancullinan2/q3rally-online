textures/qrally2/sand3
{
	surfaceparm SURF_DIRT

	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/qrally2/sand3.jpg
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}

