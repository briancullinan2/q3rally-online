
textures/q3r_people/male01
{
cull disable
	deformVertexes autosprite2
	{
		map textures/q3r_people/male01.tga
		blendFunc blend
		alphaFunc GE128
		depthWrite
		rgbGen identity
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
		depthFunc equal
	}

}


